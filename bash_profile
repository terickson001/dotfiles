export PATH="/home/tyler/.linuxbrew/bin:$PATH"
export MANPATH="/home/tyler/.linuxbrew/share/man:$MANPATH"
export INFOPATH="/home/tyler/.linuxbrew/share/info:$INFOPATH"

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

. ~/.profile
